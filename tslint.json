{
  "extends": [
    "tslint-config-prettier"
  ],
  "rules": {
    "class-name": true,
    "curly": false,
    "member-ordering": [
      true,
      "statics-first"
    ],
    "no-arg": true,
    "no-construct": true,
    "no-duplicate-variable": true,
    "no-empty": true,
    "no-eval": true,
    "no-unused-expression": true,
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "variable-name": false
  }
}
